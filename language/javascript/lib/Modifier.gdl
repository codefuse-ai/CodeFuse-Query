        return true
    }
}

/**
 * Determines whether a node kind is a modifier kind.
 */
pub fn isModifierKind(kind: int) -> bool {
    [
        {126},  // AbstractKeyword
        {131},  // AsyncKeyword
        {85},   // ConstKeyword
        {135},  // DeclareKeyword
        {88},   // DefaultKeyword
        {93},   // ExportKeyword
        {121},  // PrivateKeyword
        {122},  // ProtectedKeyword
        {123},  // PublicKeyword
        {158},  // OverrideKeyword
        {144},  // ReadonlyKeyword
        {124},  // StaticKeyword
    ]
}
