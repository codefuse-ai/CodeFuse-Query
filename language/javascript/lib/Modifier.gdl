/**
 * @filename: Statement
 * @brief: Provides classes and predicates for working with JavaScript / TypeScript modifiers
 */

/**
 * A modifier.
 */
schema Modifier extends Token {
}

impl Modifier {
    @data_constraint
    @inline
    fn __all__(db: JavascriptDB) -> *Modifier {
        for (tmp in Token(db)) {
            if (isModifier(Node {id: tmp.id})) {
                yield Modifier {
                    id : tmp.id
                }
            }
        }
    }
}

